<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.klis.ordinarium">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/MyAppCompat">
        <activity android:name="com.example.klis.ordinarium.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.klis.ordinarium.ReadActivity" />
        <activity android:name="com.example.klis.ordinarium.SpeakActivity">
            <!--android:configChanges="orientation|keyboardHidden">-->
        </activity>
    </application>

</manifest>